*{
  font-family: 'Open Sans', 'sans-serif','roboto';
  box-sizing: border-box;
}

.date-container{
  display:flex;
  width: 100%;
  height:120px;
}


.day{
  font-size: 10px;
  border-radius: 20%;
    width:80px;
    height: 80px;
    border: 1px solid #000;
  text-align: center;
  line-height: 80px;
  margin: 10px;

}

.day.active {
background-color: #ff049f;
    color: #fff;
} 




